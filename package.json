{
  "name": "studio-city-kendo-dojo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p ${PORT:-8080}",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.4",
    "@ericz1803/react-google-calendar": "^4.2.2",
    "@formspree/react": "^2.2.3",
    "@headlessui/react": "^1.3.0",
    "@heroicons/react": "^1.0.2",
    "next": "^11.0.1",
    "next-seo": "^4.26.0",
    "next-themes": "^0.0.15",
    "next-transpile-modules": "^9.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-stickynode": "^3.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "@types/node-fetch": "^2.5.11",
    "@types/react": "17.0.14",
    "@types/react-google-recaptcha": "^2.1.2",
    "@types/react-stickynode": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.31.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.1",
    "postcss": "^8.3.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.4",
    "typescript": "4.3.5"
  }
}
